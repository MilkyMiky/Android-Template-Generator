<idea-plugin>
    <id>com.dev.mike.templategenerator</id>
    <name>Template Generator</name>
    <vendor email="" url=""></vendor>

    <description>Plugin for auto generating code of android architectures.</description>

    <idea-version since-build="141"/>

    <depends>com.intellij.modules.lang</depends>
    <depends>org.jetbrains.kotlin</depends>

    <extensions defaultExtensionNs="com.intellij">

    </extensions>

    <actions>
        <group id="org.jetbrains.tutorials.actions.GroupedActions" text="Project Template" popup="true">
            <add-to-group group-id="NewGroup" anchor="first"/>

            <action id="ProjectTemplate"
                    class="ui.project.ProjectAction"
                    text="Project template"
                    description="Project template"/>

            <action id="MVITemplate"
                    class="ui.mvi.MVIAction"
                    text="MVI template"
                    description="Create MVI Template"/>
        </group>
    </actions>

</idea-plugin>